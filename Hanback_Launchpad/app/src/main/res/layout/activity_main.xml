<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/layout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="dankook.mse.hanbacklauncher.MainActivity"
    android:gravity="center"
    android:orientation="vertical"
    android:weightSum="1">


        <Button
            android:id="@+id/bgm_button1"
            android:layout_width="176dp"
            android:layout_height="wrap_content"
            android:focusable="true"
            android:text="Dark Hiphop" />

        <Button
            android:id="@+id/bgm_button2"
            android:layout_width="176dp"
            android:layout_height="wrap_content"
            android:focusable="true"
            android:text="dreamy" />

        <Button
            android:id="@+id/bgm_button3"
            android:layout_width="176dp"
            android:layout_height="wrap_content"
            android:focusable="true"
            android:text="electro" />

        <Button
            android:id="@+id/bgm_button4"
            android:layout_width="176dp"
            android:layout_height="wrap_content"
            android:focusable="true"
            android:text="hiphop lead" />
        <Button
            android:id="@+id/bgm_button5"
            android:layout_width="176dp"
            android:layout_height="wrap_content"
            android:focusable="true"
            android:text="trap" />
        <Button
            android:id="@+id/bgm_stop"
            android:layout_width="176dp"
            android:layout_height="wrap_content"
            android:focusable="true"
            android:text="Stop" />


</LinearLayout>
